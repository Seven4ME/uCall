<configuration xmlns="urn:hornetq"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="urn:hornetq /schema/hornetq-jms.xsd">
   <!--the connection factory used by the example-->
   <connection-factory name="ConnectionFactory">
      <connectors>
         <connector-ref connector-name="netty-connector"/>
      </connectors>
      <entries>
         <entry name="ConnectionFactory"/>
      </entries>
   </connection-factory>

   <!--the topic used by the example-->
   <topic name="sip1001">
      <entry name="/topic/sip1001"/>
   </topic>

   <queue name="messages_ctrl">
      <entry name="/queue/messages_ctrl"/>
   </queue>

   <queue name="messages_sip101">
      <entry name="/queue/messages_sip101"/>
   </queue>

   <queue name="messages_sip102">
      <entry name="/queue/messages_sip102"/>
   </queue>

   <queue name="messages_sip103">
      <entry name="/queue/messages_sip103"/>
   </queue>

   <queue name="messages_sip104">
      <entry name="/queue/messages_sip104"/>
   </queue>

   <queue name="messages_sip105">
      <entry name="/queue/messages_sip105"/>
   </queue>

</configuration>
